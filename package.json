    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "morgan": "1.7.0",
    "node-sass": "3.8.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "6.1.0",
    "pg-hstore": "2.3.2",
    "pug": "2.0.0-beta5",
