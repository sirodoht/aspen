{
  "name": "aspen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run watchify & npm run sasswatch & nodemon ./back/app.js",
    "postinstall": "bower install",
    "test": "jshint app.js && jshint front/scripts/app.js && mocha -b test/index.js",
    "browserify": "browserify front/scripts/app.js -o front/static/script.js",
    "watchify": "watchify front/scripts/app.js -v --poll 100ms -o front/static/script.js",
    "sass": "node-sass --include-path scss front/styles/boot.scss front/static/style.css",
    "sasswatch": "nodemon -e scss -x \"npm run sass\"",
    "build": "npm run browserify && npm run sass",
    "deploy": "npm run build && node ./back/app.js"
  },
  "engines": {
    "node": "4.x",
    "npm": ">=3.3.x"
  },
  "dependencies": {
    "bcrypt": "0.8.5",
    "bluebird": "3.2.1",
    "body-parser": "1.14.2",
    "bower": "1.7.7",
    "browserify": "13.0.0",
    "config": "1.19.0",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "jade": "1.11.0",
    "morgan": "1.6.1",
    "node-sass": "3.4.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "4.4.4",
    "pg-hstore": "2.3.2",
    "sequelize": "3.19.1",
    "serve-favicon": "2.3.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "jshint": "2.9.1",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "request": "2.69.0"
  }
}
